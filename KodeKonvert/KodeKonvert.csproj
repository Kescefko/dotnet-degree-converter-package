<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <!-- <OutputPath>bin/</OutputPath> -->
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>KodeKonvert</PackageId>
    <Title>Kode Kloud Temp Converter</Title>
    <Version>1.0.3</Version>
    <Authors>Kescefko</Authors>
    <Description>A simple C# temperature converter package.</Description>
    <PackageTags>temperature, converter, celsius, fahrenheit, kelvin</PackageTags>
  </PropertyGroup>

</Project>
